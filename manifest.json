{

  "manifest_version": 2,
  "name": "Kittify",
  "version": "1.0",
  "description": "Lets you turn all pictures into cute kitties!",
  "homepage_url": "https://github.com/panagiotisjones/Kittify",
  "icons": {
    "48": "icons/beasts-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/kitty.jpg",
    "default_title": "Kittify",
    "default_popup": "popup/popup.html"
  },

  "web_accessible_resources": [
    "cats/kitty1.jpg",
    "cats/kitty2.jpg",
    "cats/kitty3.jpg",
    "cats/kitty4.jpg",
    "cats/kitty5.jpg",
    "cats/kitty6.jpg",
    "cats/kitty7.jpg",
    "cats/kitty8.jpg",
    "cats/kitty9.jpg"
  ]

}
